{
  "name": "@myacademic/react-quiz-component",
  "version": "0.2.3",
  "description": "React Quiz Component",
  "main": "lib/Quiz.js",
  "scripts": {
    "dev": "concurrently \"npm run lib:watch\" \"npm run docs\"",
    "lib": "babel src/lib -d lib --copy-files",
    "lib:watch": "babel src/lib -w -d lib --copy-files",
    "docs": "webpack-dev-server --mode development",
    "docs:prod": "webpack --mode production",
    "build": "./node_modules/.bin/webpack --mode production",
    "make": "npm run lib && webpack --mode production",
    "pmake": "babel src/lib -d src/lib && npm run make"
  },
  "keywords": [
    "react",
    "reactjs",
    "react-quiz-component",
    "react-quiz",
    "quiz"
  ],
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.5",
    "concurrently": "^3.5.1",
    "css-loader": "^3.4.2",
    "html-webpack-plugin": "^5.5.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "style-loader": "^0.21.0",
    "webpack": "^5.20.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.13.3"
  },
  "author": "Academic EDM",
  "homepage": "https://github.com/AcademicEDM/edm-quiz",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:AcademicEDM/edm-quiz.git",
    "directory": "@AcademicEDM/edm-quiz"
  },
  "dependencies": {
    "dompurify": "^2.0.8",
    "marked": "^0.7.0"
  }
}
